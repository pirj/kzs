<div class="container">
  <ul class="row top-pannel clearfix">
    <li class="col-md-2">
      <%= link_to " назад", :back, :class => "btn btn-sm back-btn" %>

      <!--<%= link_to image_tag('create_copy.png'), copy_document_path(@document), :class => "btn btn-success", :title => "Создать по образу и подобию" %>-->
      <!--<%= link_to "pdf", document_path(@document, format: "pdf"), :target => '_blank', :class => "btn btn-success" %>-->

    </li>
    <li class="col-md-2">ТИП: <b>ПИСЬМО</b></li>

    <li class="col-md-3">
      Статус:
      <%=  document_status(@document) %>
      <!--<b class="btn-available btn dropdown" data-toggle="dropdown">Подписан</b>-->
      <ul class="dropdown-menu" role="menu">

        <div id="j-status">
        <li class="time">Последий статус назначен:
          <span class="date"><%=  document_status_date(@document) %></span>
          <div class="progress">
            <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="<%=  document_status_progress(@document) %>" aria-valuemin="0" aria-valuemax="100" style="width: <%=  document_status_progress(@document) %>%">
              <span class="sr-only"><%=  document_status_progress(@document) %>% Complete (success)</span>
            </div>
          </div>
        </li>
        <!--<li><a href="#" class="icon-ok">Отправить</a></li>-->
        <li class="status-history"><a href="#" class="icon-clock">История статусов</a></li>
        <li class="dropdown-arr"></li>
        <li><a href="#" class="icon-block-1">Отмена</a></li>

        </div>

        <div id="j-history">
          <li><span class='title icon-clock'>История назначения стстусов</span></li><li><span><a class='btn btn-sm back-btn pull-left' href='#'>Назад</a></span></li>
          <%= document_status_list(@document) %>
        </div>

      </ul>
    </li>





    <li class="col-md-4 pull-right">
      <%= render 'shared/user' %>
    </li>
  </ul>
  <h1><%= @document.title %></h1>
  <ol class="breadcrumb">
    <li><%= link_to organization_full_title(@document.sender_organization_id), organization_path(@document.sender_organization_id) %></li>
    <li><%= link_to organization_full_title(@document.organization_id), organization_path(@document.organization_id) %></li>
  </ol>
</div>







<div class="container">
  <div class="col-md-6">

    <ul class="doc-info">
      <li>
        <span class="title">Номер и дата:</span><span class="value"><a href="#"><%= @document.id %></a> / <%= if @document.sent then @document.sent_date.strftime('%d.%m.%Y') else 'Дата не присвоена' end %></span>
      </li>
      <li>
        <span class="title valign-mid">Исполнитель:</span>

        <div class="user value">
          <span class="photo">

            <img alt="" src="data:image/png;base64," />
          </span>
          <a href="#" class="name" data-toggle="dropdown"><%= user_with_full_name(@document.executor_id)%>  </a>
          <!--<ul class="dropdown-menu" role="menu">-->
            <!--<li class="title"><a href="#">Святослав Заря</a> Инженер ПТО, ООО «Циклон»</li>-->
            <!--<li class="photo">-->

            <!--</li>-->
            <!--<li><a href="#" class="icon-ok">Написать соообщение</a></li>-->
            <!--<li><a href="#" class="icon-clock">Личная страница</a></li>-->
            <!--<li class="dropdown-arr"></li>-->
            <!--<li><a href="#" class="icon-block-1">Отмена</a></li>-->
          <!--</ul>-->
        </div>
      </li>
      <li><span class="title valign-mid">Отправитель:</span>

        <div class="user value"><span class="photo">
          <!--<img src="img/user2.jpg" alt="user">-->
          <img alt="" src="data:image/png;base64," />
        </span><a href="#" class="name"><%= user_with_full_name(@document.user_id)%> </a></div>
      </li>
      <% unless @document.document_attachments.blank? %>
      <li class="doc-format"><span class="title">Приложения:</span>

            <ul class="value doc-format-list">
              <% @document.document_attachments.each do |a| %>
                  <li> <i></i> <%= link_to a.attachment_file_name, a.attachment.url(:original, false), :target=> '_blank' %></li>
              <% end %>
            </ul>

      </li>

      <% end %>



    </ul>


  </div>

  <div class="col-md-6">

    <div class="view-main-doc">
      <iframe style="width: 400px; height: 520px;" src="/documents/<%= @document.id %>.pdf"/>
      </iframe>

    </div>


    <br><br>




  </div>
  <% if @document.document_type == 'writ' && @document.task_list.present? %>
      <h4 class="col-md-12">Чек лист</h4>

      <div class="container" style="margin-left: 16px;">

        <%= render 'document_tasks' %>
        <%= render 'document_statements' %>

      </div>
  <% end %>







  <% unless @document.documents.blank? %>
      <h4 class="col-md-12">Прикрепленные документы</h4>
      <div class="attachments-carousel attachments">
        <span  class="icon-left-open">&nbsp;</span>
        <ul class="col-md-12">



          <% @document.documents.each do |d| %>
              <li class="attachment">
                <span>
                  <%= link_to d, :target => '_blank' do %>

                  <img alt="" src="data:image/png;base64," />

                  <% end  %>

                  <%= link_to d.title, d %>
                </span>
              </li>
          <% end %>

        </ul>
        <span href="" class="icon-right-open">&nbsp;</span>
      </div>

  <% end %>


  <div class="attachments-controls">
    <%= render 'actions_show' %>
  </div>



  <% unless @conversation.nil? || @conversation_documents.count < 1 %>
      <div class="container">
        <h3>История переписки
         <!-- <%= @conversation.documents.count %> писем:-->
        </h3>
        <%= render 'table', :@documents => @conversation_documents %>
      </div>
  <% end %>
</div>