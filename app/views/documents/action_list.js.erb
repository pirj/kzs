
<% if @many %>
    $('.js-update-element').append('<%= j (render 'documents/action_list', prepare: @prepare, approve: @approve,
                                                                       send_document: @send_document ) %>');
<% else %>
    $('.js-update-element').append('<%= j (render 'documents/action_list', document_id: @document_id, show: @show, create_draft: @create_draft,
                                                                       edit: @edit, prepare: @prepare, approve: @approve,
                                                                       send_document: @send_document, reply: @reply, copy: @copy ) %>');
<% end %>
