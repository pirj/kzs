$('table').html("<%=j render :partial => 'table', :@statements => @statements %>");

$(".dynamic-table tbody tr").click(function(){
		var self = this;
		var controller = $('table').attr('id')
    appendTr(controller, self)
})


$(".dynamic-table tbody td.not_this").click(function(e){
    e.stopPropagation()
})
