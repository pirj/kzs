<h5>
  меню с вкладками
</h5>
<div class="bs-example">
  <ul class="nav nav-tabs">
    <li class="active"><a href="#main" data-toggle="tab">Общие</a></li>
    <li><a href="#details" data-toggle="tab">Реквизиты</a></li>
    <li><a href="#users" data-toggle="tab">Сотрудники</a></li>
    <li><a href="#licenses" data-toggle="tab">Лицензии</a></li>
  </ul>

  <div class="tab-content">
    <div class="tab-pane active" id="main">Main information</div>
    <div class="tab-pane" id="details">:) only smile...</div>
  </div>
</div>

<div class="highlight">
  <ul class="nav nav-pills lib-control-tabs">
    <li><a href="#tab-menu-slim" data-toggle="tab">slim</a></li>
    <li><a href="#tab-menu-html" data-toggle="tab">html</a></li>
  </ul>
  <!-- Tab panes -->
  <div class="tab-content">
    <div class="tab-pane" id="tab-menu-slim">
      <pre class="prettyprint linenums lang-ruby"><%= html_escape %q{
  ul.nav.nav-tabs
    li.active = link_to 'Общие', '#main', data: { toggle: 'tab' }
    li = link_to 'Реквизиты', '#details', data: { toggle: 'tab' }
    li = link_to 'Сотрудники', '#users', data: { toggle: 'tab' }
    li = link_to 'Лицензии', '#licenses', data: { toggle: 'tab' }

    .tab-content
      .tab-pane.active#main
      .tab-pane#details
      }%></pre>
    </div>

    <div class="tab-pane" id="tab-menu-html">
      <pre class="prettyprint linenums lang-html"><%= html_escape %q{
  <ul class="nav nav-tabs">
    <li class="active"><a href="#main" data-toggle="tab">Общие</a></li>
    <li><a href="#details" data-toggle="tab">Реквизиты</a></li>
    <li><a href="#users" data-toggle="tab">Сотрудники</a></li>
    <li><a href="#licenses" data-toggle="tab">Лицензии</a></li>
  </ul>

  <div class="tab-content">
    <div class="tab-pane active" id="main">Main information</div>
    <div class="tab-pane" id="details">:) only smile...</div>
  </div>
      }%></pre>
    </div>
  </div>
</div>